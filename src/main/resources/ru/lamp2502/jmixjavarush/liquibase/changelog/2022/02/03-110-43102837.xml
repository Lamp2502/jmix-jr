<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="jmixjavarush">
        <addColumn tableName="CONTRACT">
            <column name="BODY_" type="CLOB"/>
            <column name="CREATED_BY" type="VARCHAR(255)"/>
            <column name="CREATED_DATE" type="DATETIME"/>
            <column name="HAT" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_BY" type="VARCHAR(255)"/>
            <column name="LAST_MODIFIED_DATE" type="DATETIME"/>
            <column name="NUMBER_" type="VARCHAR(255)"/>
            <column name="PERFORMER" type="VARCHAR(255)"/>
            <column name="STATUS" type="VARCHAR(255)"/>
            <column name="TITEL" type="VARCHAR(255)"/>
        </addColumn>
    </changeSet>
    <changeSet id="2" author="jmixjavarush">
        <addColumn tableName="DOCUMENT">
            <column name="BODY_" type="CLOB"/>
